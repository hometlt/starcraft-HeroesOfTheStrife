<Desc>
	<Frame type="SelectionUnitStatusPanel" name="GameButtonProgressBarCountdownTemplate">
		<Anchor relative="$parent" />
		<SpecificUnit val="_HERO_NAME_" />
		<Frame type="UnitStatusFrame" name="UnitStatusFrame0">
			<Anchor relative="$parent" />
			<Frame type="UnitStatusBehaviorIcon" name="UnitStatusBehaviorIcon">
				<Anchor relative="$parent" />
				<Behavior val="_BEHAVIOR_NAME_" />
