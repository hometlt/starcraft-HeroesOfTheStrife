	<!--                         -->
	<!--        Katarina         -->
	<!--                         -->
	<Frame type="SelectionUnitStatusPanel" name="KatarinaWeaponController">
		<Anchor relative="$parent"/>
		<SpecificUnit val="HeroKatarina" />
		<Frame type="UnitStatusFrame" name="UnitStatusFrame0">
			<Anchor relative="$parent" />
			<Frame type="UnitStatusValidatorAnimElement" name="CarriedWeaponVisibilityController">
				<Anchor relative="$parent"/>
				<Validator val="HeroKatarinaWeaponAutorifle@TargetHasWeaponCarried"/>
				<Validator val="HeroKatarinaWeaponFlamethrower@TargetHasWeaponCarried"/>
				<Validator val="HeroKatarinaWeaponGrenadeLauncher@TargetHasWeaponCarried"/>
				<Validator val="HeroKatarinaWeaponSnipe@TargetHasWeaponCarried"/>
				<StateGroup name="Visibility">
					<DefaultState val="NoState"/>
					<State name="CarriedAutorifle">
						<When type="AnimationState" UnitStatusValidatorState="HeroKatarinaWeaponAutorifle@TargetHasWeaponCarried" />
						<When type="Property" frame="$parent/$parent/$parent/$parent/NormalImage" Texture="Assets\Icons\Hero_Katarina\Katarina_Weapon_Autorifle_Carried.dds" index="0"/>
						<Action type="SetProperty" frame="$parent/$parent/$parent/$parent/SelectedEffect" Visible="True"/>
					</State>
					<State name="CarriedFlamethrower">
						<When type="AnimationState" UnitStatusValidatorState="HeroKatarinaWeaponFlamethrower@TargetHasWeaponCarried" />
						<When type="Property" frame="$parent/$parent/$parent/$parent/NormalImage" Texture="Assets\Icons\Hero_Katarina\Katarina_Weapon_Flamethrower_Carried.dds" index="0"/>
						<Action type="SetProperty" frame="$parent/$parent/$parent/$parent/SelectedEffect" Visible="True"/>
					</State>
					<State name="CarriedGrenadeLauncher">
						<When type="AnimationState" UnitStatusValidatorState="HeroKatarinaWeaponGrenadeLauncher@TargetHasWeaponCarried" />
						<When type="Property" frame="$parent/$parent/$parent/$parent/NormalImage" Texture="Assets\Icons\Hero_Katarina\Katarina_Weapon_GrenadeLauncher_Carried.dds" index="0"/>
						<Action type="SetProperty" frame="$parent/$parent/$parent/$parent/SelectedEffect" Visible="True"/>
					</State>
					<State name="CarriedSnipe">
						<When type="AnimationState" UnitStatusValidatorState="HeroKatarinaWeaponSnipe@TargetHasWeaponCarried" />
						<When type="Property" frame="$parent/$parent/$parent/$parent/NormalImage" Texture="Assets\Icons\Hero_Katarina\Katarina_Weapon_Snipe_Carried.dds" index="0"/>
						<Action type="SetProperty" frame="$parent/$parent/$parent/$parent/SelectedEffect" Visible="True"/>
					</State>
				</StateGroup>
			</Frame>
			<Frame type="UnitStatusValidatorAnimElement" name="EquippedWeaponVisibilityController">
				<Validator val="HeroKatarinaWeaponAutorifle@TargetHasWeaponEquipped"/>
				<Validator val="HeroKatarinaWeaponFlamethrower@TargetHasWeaponEquipped"/>
				<Validator val="HeroKatarinaWeaponGrenadeLauncher@TargetHasWeaponEquipped"/>
				<Validator val="HeroKatarinaWeaponSnipe@TargetHasWeaponEquipped"/>
				<StateGroup name="Visibility">
					<DefaultState val="NoState"/>
					<State name="EquippedAutorifle">
						<When type="AnimationState" UnitStatusValidatorState="HeroKatarinaWeaponAutorifle@TargetHasWeaponEquipped" />
						<When type="Property" frame="$parent/$parent/$parent/$parent/NormalImage" Texture="Assets\Icons\Hero_Katarina\Katarina_Weapon_Autorifle_Carried.dds" index="0"/>
						<Action type="SetProperty" frame="$parent/$parent/$parent/$parent/SelectedEffectSpecial" Visible="True"/>
					</State>
					<State name="EquippedFlamethrower">
						<When type="AnimationState" UnitStatusValidatorState="HeroKatarinaWeaponFlamethrower@TargetHasWeaponEquipped" />
						<When type="Property" frame="$parent/$parent/$parent/$parent/NormalImage" Texture="Assets\Icons\Hero_Katarina\Katarina_Weapon_Flamethrower_Carried.dds" index="0"/>
						<Action type="SetProperty" frame="$parent/$parent/$parent/$parent/SelectedEffectSpecial" Visible="True"/>
					</State>
					<State name="EquippedGrenadeLauncher">
						<When type="AnimationState" UnitStatusValidatorState="HeroKatarinaWeaponGrenadeLauncher@TargetHasWeaponEquipped" />
						<When type="Property" frame="$parent/$parent/$parent/$parent/NormalImage" Texture="Assets\Icons\Hero_Katarina\Katarina_Weapon_GrenadeLauncher_Carried.dds" index="0"/>
						<Action type="SetProperty" frame="$parent/$parent/$parent/$parent/SelectedEffectSpecial" Visible="True"/>
					</State>
					<State name="EquippedSnipe">
						<When type="AnimationState" UnitStatusValidatorState="HeroKatarinaWeaponSnipe@TargetHasWeaponEquipped" />
						<When type="Property" frame="$parent/$parent/$parent/$parent/NormalImage" Texture="Assets\Icons\Hero_Katarina\Katarina_Weapon_Snipe_Carried.dds" index="0"/>
						<Action type="SetProperty" frame="$parent/$parent/$parent/$parent/SelectedEffectSpecial" Visible="True"/>
					</State>
				</StateGroup>
			</Frame>
		</Frame>
	</Frame>
